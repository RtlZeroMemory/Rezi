{
  "name": "@rezi-ui/core",
  "version": "0.1.0-alpha.29",
  "description": "Runtime-agnostic TypeScript core for Rezi (widgets, layout, routing, drawlists, event parsing).",
  "license": "Apache-2.0",
  "homepage": "https://rezitui.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RtlZeroMemory/Rezi.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/RtlZeroMemory/Rezi/issues"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json --pretty false",
    "test": "CONCURRENCY=$(node -p \"parseInt(process.versions.node)>=19?'--test-concurrency=1':''\") && node --test $CONCURRENCY $(find dist -type f -path '*/__tests__/*.test.js' | LC_ALL=C sort)"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./all": {
      "types": "./dist/all.d.ts",
      "default": "./dist/all.js"
    },
    "./abi": {
      "types": "./dist/abi.d.ts",
      "default": "./dist/abi.js"
    },
    "./backend": {
      "types": "./dist/backend.d.ts",
      "default": "./dist/backend.js"
    },
    "./composition": {
      "types": "./dist/composition.d.ts",
      "default": "./dist/composition.js"
    },
    "./cursor": {
      "types": "./dist/cursor/index.d.ts",
      "default": "./dist/cursor/index.js"
    },
    "./debug": {
      "types": "./dist/debug/index.d.ts",
      "default": "./dist/debug/index.js"
    },
    "./drawlist": {
      "types": "./dist/drawlist/index.d.ts",
      "default": "./dist/drawlist/index.js"
    },
    "./drawApi": {
      "types": "./dist/drawApi.d.ts",
      "default": "./dist/drawApi.js"
    },
    "./events": {
      "types": "./dist/events.d.ts",
      "default": "./dist/events.js"
    },
    "./forms": {
      "types": "./dist/forms/index.d.ts",
      "default": "./dist/forms/index.js"
    },
    "./keybindings": {
      "types": "./dist/keybindings/index.d.ts",
      "default": "./dist/keybindings/index.js"
    },
    "./layout": {
      "types": "./dist/layout/index.d.ts",
      "default": "./dist/layout/index.js"
    },
    "./protocol": {
      "types": "./dist/protocol/index.d.ts",
      "default": "./dist/protocol/index.js"
    },
    "./router": {
      "types": "./dist/router/index.d.ts",
      "default": "./dist/router/index.js"
    },
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "default": "./dist/theme/index.js"
    },
    "./terminalCaps": {
      "types": "./dist/terminalCaps.d.ts",
      "default": "./dist/terminalCaps.js"
    },
    "./terminalProfile": {
      "types": "./dist/terminalProfile.d.ts",
      "default": "./dist/terminalProfile.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "default": "./dist/testing/index.js"
    },
    "./ui": {
      "types": "./dist/ui.d.ts",
      "default": "./dist/ui.js"
    },
    "./widgets": {
      "types": "./dist/widgets/index.d.ts",
      "default": "./dist/widgets/index.js"
    }
  },
  "files": ["dist/", "README.md", "!dist/**/__tests__/**", "!dist/**/__e2e__/**"],
  "engines": {
    "node": ">=18",
    "bun": ">=1.3.0"
  },
  "devDependencies": {
    "@rezi-ui/testkit": "0.1.0-alpha.29"
  }
}
